{
    "manifest_version": 3,
    "name": "Network Logger",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "downloads",
      "devtools"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "devtools_page": "devtools.html"
  }
  